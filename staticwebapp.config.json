{  "routes": [
    {
      "route": "/api/*",
      "methods": ["PUT", "POST", "PATCH", "DELETE"],
      "allowedRoles": ["administrator"],
      "statusCode": 404,
      "rewrite": "/.auth/login/github",
       "headers": {
            "Cache-Control": "public, max-age=604800, immutable"
        }
    }
  ],
    "navigationFallback": {
        "rewrite": "/index.html"
    }
}
