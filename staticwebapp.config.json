{
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "PUT",
        "POST"
      ],
      "allowedRoles": [
        "administrator"
      ],
      "statusCode": 404,
      "rewrite": "/.auth/login/github",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable"
      }
    },
    {
      "route": "/profile/codeEdit",
      "methods": [
        "GET"
      ],
      "rewrite": "rewrite",
      "redirect": "redirect",
      "statusCode": 200,
      "allowedRoles": [
        "anonymous",
        " admin"
      ],
      "headers": {
        "name1": "value1",
        "name2": "value2"
      }
    },
    {
      "route": "newName-codeEditTest",
      "rewrite": "/item/123",
      "statusCode": 201,
      "headers": {
        "name1-codeEdit": "value1-codeEdit"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}